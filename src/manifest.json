{
  "name": "Button Icon Preference - Shield Study Example",
  "description": "__MSG_extensionDescription__",
  "version": "2.0.0",
  "manifest_version": 2,
  "applications": {
    "gecko": {
      "id": "button-icon-preference@shield.mozilla.org",
      "strict_min_version": "59.0"
    }
  },
  "default_locale": "en-US",
  "experiment_apis": {
    "feature": {
      "schema": "./privileged/feature/schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "script": "./privileged/feature/api.js",
        "paths": [["feature"]]
      }
    },
    "shieldUtils": {
      "schema": "./privileged/shieldUtils/schema.json",
      "parent": {
        "scopes": ["addon_parent"],
        "script": "./privileged/shieldUtils/api.js",
        "paths": [["shieldUtils"]]
      }
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "48": "icon.png"
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": "icons/isolatedcorndog.svg",
    "default_title": "__MSG_buttonTitle__"
  }
}
